{
  "manifest_version": 3,
  "name": "OwO",
  "version": "1.0.0",
  "description": "A basic example extension with only required keys",
  "action": {
    "default_popup": "hello.html"
  },
  "content_scripts":[
    {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
     }
  ],
  "permissions": ["scripting", "activeTab", "tabs"],
  "icons":{
    "16": "images/icon-48.png",
    "48": "images/icon-48.png",
    "128": "images/icon-48.png"
  }
}

